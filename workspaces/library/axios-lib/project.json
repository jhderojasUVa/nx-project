{
  "name": "@jhderojasUVa/axios-lib",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "workspaces/library/axios-lib/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/workspaces/library/axios-lib",
        "main": "workspaces/library/axios-lib/src/index.ts",
        "tsConfig": "workspaces/library/axios-lib/tsconfig.lib.json",
        "assets": [
          "workspaces/library/axios-lib/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../../coverage/workspaces/library/axios-lib"
      }
    }
  }
}